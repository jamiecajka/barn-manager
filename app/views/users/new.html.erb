
<div class="small-3 small-centered columns">
    <div class="card">
      <% if user_signed_in? %>
        <h2 class="nav_buttons"> <%= link_to 'Sign Out', destroy_user_session_path, method: :delete %> </h2>
        <h2 class="nav_buttons"> <%= link_to 'Edit Account', edit_user_registration_path %></h2>
        <h2 class="nav_buttons"> <%= link_to 'Owner Info', user_path(current_user) %>
      <% else %>
        <h2 class="nav_buttons"><%= link_to 'Sign In', new_user_session_path %></h2>
        <h2 class="nav_buttons"><%= link_to 'Sign Up', new_user_registration_path %></h2>
      <% end %>
    </div>
  </div>
</div>
